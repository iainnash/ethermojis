{
    "name": "Ethermoji - Ethereum data to Emoijs",
    "short_name": "Ethermoji",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Make all ethereum txn hashes emojis on etherscan",
    "author": "Iain Nash",
    "content_scripts": [{
        "matches": ["https://*.etherscan.io/*", "https://etherscan.io/*"],
        "all_frames": true,
        "js": ["content_script.js"],
        "run_at": "document_end"
    }]
}
